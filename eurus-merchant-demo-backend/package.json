{
  "name": "merchant-admin-demo",
  "version": "1.0.0",
  "description": "merchant-admin-demo",
  "_main": "index.js",
  "scripts": {
    "killServer": "./node_modules/.bin/pm2 kill ",
    "prebuild": "npm run tsc:all",
    "build": "npm run wpDev && npm run wpLocal",
    "tsc:all": "npm run tsc:merchantAdmin",
    "tsc:merchantAdmin": "./node_modules/.bin/tsc -p  src/server/merchantAdmin",
    "wpDev": "npm run wpDev:merchantAdmin ",
    "wpDev:merchantAdmin": "./node_modules/.bin/webpack --config webpack/merchantAdmin/merchantAdmin.dev.js",
    "wpLocal": "npm run wpLocal:merchantAdmin ",
    "wpLocal:merchantAdmin": "./node_modules/.bin/webpack --config webpack/merchantAdmin/merchantAdmin.local.js",
    "dev": "npm run killServer && npm run dev:merchantAdmin",
    "dev:merchantAdmin": "./node_modules/.bin/pm2  start dist/webpack/dev/merchantAdmin/bundle.js --name merchantAdmin",
    "local": "npm run killServer && npm run local:merchantAdmin",
    "local:merchantAdmin": "./node_modules/.bin/pm2  start dist/webpack/local/merchantAdmin/bundle.js --name merchantAdmin"
  },
